{% extends '@EasyAdmin/page/content.html.twig' %}

{% block content_title %}The Title of the Page{% endblock %}
{% block page_actions %}
    <a class="btn btn-primary" href="#">Some Action</a>
{% endblock %}

{% block main %}
    <table class="datagrid">
        <thead>
        <tr>
            <td>Label</td>
            <td>Description</td>
        </tr>
        </thead>
        <tbody>
        {% for data in test %}
            <tr>
                <td>{{ data.label }}</td>
                <td>{{ data.description }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}